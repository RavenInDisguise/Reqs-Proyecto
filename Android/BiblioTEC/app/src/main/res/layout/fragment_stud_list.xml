<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listaEstudiantes"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.studListFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/admin_menu_title3"
            style="@style/menuTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Estudiantes"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ListView
            android:id="@+id/lista_estudiantes"
            android:layout_width="350dp"
            android:layout_height="663dp"
            android:layout_marginStart="28dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/admin_menu_title3" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>